					if(number){
						num = number;
					}else{
						num = 10;
					}
				}
				if(arr.length < num){
					if(brand_ids){
						arr = brand_ids + ','+brandId;
					}else{
						arr = brandId;
					}
					_this.addClass("selected");
				}else{
					alert("品牌选择不能超过"+num+"个");
				}
			}
			$("input[name='brand_ids']").val(arr);
		});
		
